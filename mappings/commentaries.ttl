@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix : <http://example.org/rules/> .

@prefix khub: <http://semanticweb.org/ontologies/khub#> .

:TriplesMap a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "jsons/commentaries.json" ;
    rml:referenceFormulation ql:JSONPath ;
    rml:iterator "$.[*]"
] .

:TriplesMap rr:subjectMap [
  rr:template "http://semanticweb.org/ontologies/khub#{_id}"
] .

:TriplesMap rr:predicateObjectMap [
  rr:predicate khub:author ;
  rr:objectMap [
    rr:template "http://semanticweb.org/ontologies/khub#{createdBy}"
  ]
] .

:TriplesMap rr:predicateObjectMap [
  rr:predicate khub:ancestor ;
  rr:objectMap [
    rr:template "http://semanticweb.org/ontologies/khub#{ancestors[*]}"
  ]
] .

:TriplesMap rr:predicateObjectMap [
  rr:predicate rdf:type ;
  rr:objectMap [
    rr:constant khub:Commentary
  ]
] .

:TriplesMap rr:predicateObjectMap [
  rr:predicate khub:title ;
  rr:objectMap [
    rml:reference "title"
  ]
] .

:TriplesMap rr:predicateObjectMap [
  rr:predicate khub:content ;
  rr:objectMap [
    rml:reference "content"
  ]
] .

:TriplesMap rr:predicateObjectMap [
  rr:predicate khub:link ;
  rr:objectMap [
    rml:reference "link"
  ]
] .

:TriplesMap rr:predicateObjectMap [
  rr:predicate khub:creationTime ;
  rr:objectMap [
    rml:reference "createdAt"
  ]
] .

:TriplesMap rr:predicateObjectMap [
  rr:predicate khub:lastUpdateTime ;
  rr:objectMap [
    rml:reference "lastUpdatedAt"
  ]
] .