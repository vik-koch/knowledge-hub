@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix : <http://example.org/rules/> .

@prefix khub: <http://semanticweb.org/ontologies/khub#> .

:TriplesMap a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "jsons/people.json" ;
    rml:referenceFormulation ql:JSONPath ;
    rml:iterator "$.[*]"
] .

:TriplesMap rr:subjectMap [
  rr:template "http://semanticweb.org/ontologies/khub#{username}"
] .

:TriplesMap rr:predicateObjectMap [
  rr:predicate owl:sameAs ;
  rr:objectMap [
    rr:template "http://semanticweb.org/ontologies/khub#{name}"
  ]
] .

:TriplesMap rr:predicateObjectMap [
  rr:predicate rdf:type ;
  rr:objectMap [
    rr:constant khub:Person
  ]
] .

:TriplesMap rr:predicateObjectMap [
  rr:predicate khub:title ;
  rr:objectMap [
    rml:reference "name"
  ]
] .